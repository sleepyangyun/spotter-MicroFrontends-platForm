{
    "name": "spotter-microfrontends-platform",
    "version": "1.0.0",
    "description": "【微前端】主应用模版，乾坤+联邦模块",
    "author": {
        "name": "Yeong"
    },
    "private": true,
    "scripts": {
        "start": " pnpm build && pnpm start:dev",
        "dev:web": "pnpm start:dev:web",
        "dev:node": "pnpm start:dev:node",
        "start:dev:web": "cross-env-shell NODE_ENV=development  ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/vite/commands/devWeb.ts",
        "start:dev:node": "nest build --webpack --path server/tsconfig.build.json --config  server/nest-cli.json --webpackPath scripts/webpack/configs/node/webpack.nest.dev.js --watch",
        "start:prod": "cross-env-shell NODE_ENV=production node dist/server.js",
        "start:test": "cross-env-shell NODE_ENV=test node dist/server.js",
        "start:dev": "cross-env-shell NODE_ENV=development node dist/server.js",
        "build": "pnpm install && pnpm build:web && pnpm build:node",
        "build:web": "cross-env-shell NODE_ENV=production ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/webpack/commands/buildWeb.ts",
        "build:web:release": "cross-env-shell NODE_ENV=production ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/webpack/commands/buildWeb.ts --release",
        "publish:web": "cross-env-shell NODE_ENV=production  ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/webpack/commands/publishWeb.ts",
        "build:web:ana": "cross-env-shell NODE_ENV=production ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/webpack/commands/buildWeb.ts --analyze",
        "build:node": "nest build --webpack --path server/tsconfig.build.json --config  server/nest-cli.json --webpackPath scripts/webpack/configs/node/webpack.nest.prod.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e-node": "jest --config server/test/jest-e2e.json",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint",
        "lint:eslint": "eslint -c .eslintrc.js --ignore-pattern '/node_modules/' --ext .js,.ts,.tsx  client/app server scripts --fix",
        "lint:stylelint": "stylelint --config .stylelintrc.js client/style/*.{css,less,scss} --fix",
        "prettier": "prettier --write",
        "check:type": "cross-env-shell NODE_ENV=production ts-node --files -P scripts/tsconfig.json -r tsconfig-paths/register scripts/webpack/commands/typeCheck.ts",
        "doc": "pnpx @compodoc/compodoc -p server/tsconfig.build.json -s",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "docker:build": "docker build . -t  192.168.8.14:8082/spotter/romerx-web:1.0.0",
        "docker:push": "docker push 192.168.8.14:8082/spotter/romerx-web:1.0.0",
        "lint-staged": "lint-staged",
        "analyze:windi": "npx windicss-analysis",
        "gen:i18n": "npx i18next",
        "updeps": "pnpm up -r -L \"@spotter/*\"",
        "prepare": "husky install",
        "preinstall": "npx only-allow pnpm"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint -c .eslintrc.js --fix"
        ],
        "*.{css,less,scss}": [
            "stylelint --config .stylelintrc.js --fix"
        ],
        "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": "^16.13.2",
        "pnpm": ">=5"
    },
    "browserslist": [
        "last 2 versions",
        "Firefox ESR",
        "> 1%",
        "ie >= 10"
    ],
